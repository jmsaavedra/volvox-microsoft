{

	"name" : "Video Processor",
	"script" : "./app.js",
	"env" : {
		"FORCE_COLOR":"true"
	},
	"error_file" : "./logs/vidprocess-app.stderr.log",
	"out_file" : "./logs/vidprocess-app.stdout.log",
	"instances" : 1,
	"min_uptime" : "20s",
	"max_restarts" : 99999,
	"merge_logs" : true,
	"exec_interpreter" : "node",
	"exec_mode" : "fork"
}